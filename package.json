  "main": "index.js",
  "scripts": {
    "start": "npm i && concurrently --kill-others \"better-npm-run start-dev\"",
    "build": "better-npm-run build",
    "test": "jest --coverage",
    "t1": "babel --plugins transform-es2015-modules-umd src --ignore __tests__ --out-dir ./dist",
    "t2": "rimraf es6 && babel ./lib -d es6"
  },
  "betterScripts": {
    "start-dev": {
      "command": "node server.js",
      "env": {
        "HOST": "localhost",
        "PORT": 8082,
        "APIHOST": "localhost",
        "APIPORT": 8082
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack.prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
